@model IEnumerable<Order>

@{
    ViewBag.Title = "Orders";
    Layout = "_AdminLayout";
}

@if (Model.Count() > 0)
{
    foreach (var item in Model.ToList())
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@item"/>
        </div>
    }
}
else
{
    <div class="alert alert-info">
        <p class="lead">There is no order yet.</p>
    </div>
}